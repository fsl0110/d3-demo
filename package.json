{
  "name": "d3-demo",
  "version": "0.1.0",
  "description": "A react Demo Project for D3",
  "author": "Frank Simon Lechner",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fsl0110/d3-demo.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:run": "yarn build && serve ./build",
    "serve": "serve ./build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "styleguide": "yarn styleguidist server",
    "styleguide:build": "yarn styleguidist build",
    "styleguide:build:run": "yarn styleguidist build && serve ./styleguide",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@types/moment": "^2.13.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "d3": "^5.9.7",
    "immer": "^3.1.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "normalize-scss": "^7.0.1",
    "react": "^16.8.6",
    "react-docgen-typescript": "^1.12.5",
    "react-dom": "^16.8.6",
    "react-faux-dom": "^4.5.0",
    "react-loading-skeleton": "^1.1.2",
    "react-router-dom": "^5.0.1",
    "react-skeleton": "^1.0.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.9",
    "@types/d3": "^5.7.2",
    "@types/enzyme": "^3.10.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/moxios": "^0.4.8",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "moxios": "^0.4.0",
    "node-sass": "^4.12.0",
    "react-docgen-typescript": "^1.12.5",
    "react-scripts": "3.0.1",
    "react-styleguidist": "^9.1.11",
    "serve": "^11.1.0",
    "source-map-explorer": "^2.0.1",
    "typescript": "3.5.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/build/",
      "!<rootDir>/styleguide/",
      "!<rootDir>/src/react-app-env.d.ts",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/pages/index.ts",
      "!<rootDir>/src/components/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
